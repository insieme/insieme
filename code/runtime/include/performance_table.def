IRT_INST_EVENT(IRT_INST_WORK_ITEM_SUSPENDED_JOIN,				"WI",	"SUSP_JOIN")
IRT_INST_EVENT(IRT_INST_WORK_ITEM_SUSPENDED_GROUPJOIN,			"WI",	"SUSP_GROUPJOIN")
