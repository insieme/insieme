	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_lambda"> 
	<xs:sequence>
		<xs:element name="type" type="T_functionTypePtr" minOccurs="1" maxOccurs="1"/>
