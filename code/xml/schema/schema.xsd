		<xs:element name="elseBody" type="T_statementPtr" minOccurs="1" maxOccurs="1"/>		
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_forStmt">
	<xs:sequence>
		<xs:element name="declaration" type="T_declarationStmtPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="body" type="T_statementPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="end" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>	
		<xs:element name="step" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_returnStmt">
	<xs:sequence>
		<xs:element name="returnExpression" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_recTypeDefinition">
	<xs:sequence>
		<xs:element name="definitions" type="T_recTypeDefs" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_recType">
	<xs:sequence>
		<xs:element name="definition" type="T_RecTypeDefinitionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="typeVariable" type="T_typeVariablePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_literal">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
	<xs:attribute name="id" type="xs:integer" use="required"/>
	<xs:attribute name="value" type="xs:string" use="required"/>
</xs:complexType>

<xs:complexType name="T_variable">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
	<xs:attribute name="id" type="xs:integer" use="required"/>
	<xs:attribute name="varId" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_markerStmt">
	<xs:sequence>
		<xs:element name="subStatement" type="T_statementPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
	<xs:attribute name="id" type="xs:integer" use="required"/>
	<xs:attribute name="markId" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_markerExpr">
	<xs:sequence>
		<xs:element name="subExpression" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
	<xs:attribute name="id" type="xs:integer" use="required"/>
	<xs:attribute name="markId" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_typeVariable">
	<xs:sequence>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
	<xs:attribute name="id" type="xs:integer" use="required"/>
	<xs:attribute name="name" type="xs:string" use="required"/>
</xs:complexType>
	
<xs:complexType name="T_tupleType">
	<xs:sequence>
		<xs:element name="elementTypeList" type="T_listTypePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_namedCompositeType">
	<xs:sequence>
		<xs:element name="entries" type="T_entries" minOccurs="1" maxOccurs="1"/>
		<xs:element name="parents" type="T_listParents" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_genType">
	<xs:sequence>
		<xs:element name="baseType" type="T_typePtr" minOccurs="0" maxOccurs="1"/>
		<xs:element name="typeParams" type="T_listTypePtr" minOccurs="0" maxOccurs="1"/>
		<xs:element name="intTypeParams" type="T_listIntTypeParamPtr" minOccurs="0" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="familyName" type="xs:string" use="required"/>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_functionType"> 
	<xs:sequence>
		<xs:element name="parameterTypeList" type="T_listTypePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="returnType" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="kind" type="xs:integer" use="required"/>
