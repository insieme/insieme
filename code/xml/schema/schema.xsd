<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

<!-- Annotation -->
<xs:complexType name="T_annotations">
	<xs:sequence>
		<xs:any minOccurs="0" maxOccurs="unbounded" processContents="skip"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_nodePtr">
	<xs:attribute name="ref" type="xs:positiveInteger" use="required"/>
</xs:complexType>

<xs:complexType name="T_typePtr">
	<xs:sequence>
		<xs:element name="typePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_listTypePtr">
	<xs:sequence>
		<xs:element name="typePtr" type="T_nodePtr" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_listIntTypeParamPtr">
	<xs:sequence>
		<xs:element name="intTypeParamPtr" type="T_nodePtr" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_parent">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
		<xs:attribute name="virtual" type="xs:boolean" use="required" />
</xs:complexType>

<xs:complexType name="T_listParents">
	<xs:sequence>
		<xs:element name="parentPtr" type="T_nodePtr" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_variablePtr">
	<xs:sequence>
		<xs:element name="variablePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_listVariablePtr">
	<xs:sequence>
		<xs:element name="variablePtr" type="T_nodePtr" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_identifierPtr">
	<xs:sequence>
		<xs:element name="identifierPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_expressionPtr">
	<xs:sequence>
		<xs:element name="expressionPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_listExpressionPtr">
	<xs:sequence>
		<xs:element name="expressionPtr" type="T_nodePtr" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_functionTypePtr">
	<xs:sequence>
		<xs:element name="functionTypePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_RecTypeDefinitionPtr">
	<xs:sequence>
		<xs:element name="recTypeDefinitionPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_declarationStmtPtr">
	<xs:sequence>
		<xs:element name="declarationStmtPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_listDeclarationStmtPtr">
	<xs:sequence>
		<xs:element name="declarationStmtPtr" type="T_nodePtr" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_lambdaExprPtr">
    <xs:sequence>
        <xs:element name="lambdaExprPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>   
</xs:complexType>

<xs:complexType name="T_callExprPtr">
	<xs:sequence>
		<xs:element name="callExprPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_lambdaDefinitionPtr">
	<xs:sequence>
		<xs:element name="lambdaDefinitionPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_statementPtr">
	<xs:sequence>
		<xs:element name="statementPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_listStatementPtr">
	<xs:sequence>
		<xs:element name="statementPtr" type="T_nodePtr" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_typeVariablePtr">
	<xs:sequence>
		<xs:element name="typeVariablePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>	
</xs:complexType>

<xs:complexType name="T_tupleTypePtr">
	<xs:sequence>
		<xs:element name="tupleTypePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>
</xs:complexType>

<!-- T_entries -->
<xs:complexType name="T_entries">
	<xs:sequence>
		<xs:element name="entry" type="T_entry" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_entry">
	<xs:sequence>
		<xs:element name="identifierPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="typePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>
</xs:complexType>

<!-- T_members -->
<xs:complexType name="T_members">
	<xs:sequence>
		<xs:element name="member" type="T_member" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_member">
	<xs:sequence>
		<xs:element name="identifierPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="expressionPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>
</xs:complexType>

<!-- T_recTypeDefs -->
<xs:complexType name="T_recTypeDefs">
	<xs:sequence>
		<xs:element name="definition" type="T_recTypeDef" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_recTypeDef">
	<xs:sequence>
		<xs:element name="typeVariablePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="typePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>
</xs:complexType>

<!-- T_definitions -->
<xs:complexType name="T_definitions">
	<xs:sequence>
		<xs:element name="definition" type="T_definition" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_definition">
	<xs:sequence>
		<xs:element name="variablePtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="lambdaPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
	</xs:sequence>
</xs:complexType>

<!-- T_guards -->
<xs:complexType name="T_guards">
    <xs:sequence>
        <xs:element name="expressionPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
        <xs:element name="expressionPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
</xs:complexType>


<!-- T_cases -->
<xs:complexType name="T_cases">
	<xs:sequence>
		<xs:element name="case" type="T_case" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<xs:complexType name="T_case">
    <xs:sequence>
        <xs:element name="expressionPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
        <xs:element name="statementPtr" type="T_nodePtr" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
</xs:complexType>

<!-- T_guardedStatements -->
<xs:complexType name="T_guardedStatements">
	<xs:sequence>
		<xs:element name="guardedStatement" type="T_guards" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<!-- T_params -->
<xs:complexType name="T_params">
	<xs:sequence>
		<xs:element name="param" type="T_variablePtr" minOccurs="0" maxOccurs="unbounded"/>
	</xs:sequence>
</xs:complexType>

<!-- All IR Node -->
<xs:complexType name="T_empty">
	<xs:sequence>
		<xs:element name="annotations" type="T_annotations" minOccurs="0"/>
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_program">
	<xs:sequence>
		<xs:element name="expressions" type="T_listExpressionPtr" minOccurs="1" maxOccurs="1"/>	
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
		<xs:attribute name="main" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_identifier">
	<xs:sequence>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
	<xs:attribute name="id" type="xs:integer" use="required"/>
	<xs:attribute name="name" type="xs:string" use="required"/>
</xs:complexType>

<xs:complexType name="T_lambdaExpr">
	<xs:sequence>
		<xs:element name="variable" type="T_variablePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="definition" type="T_lambdaDefinitionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_jobExpr">
	<xs:sequence>
		<xs:element name="declarations" type="T_listDeclarationStmtPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="guardedStatements" type="T_guardedStatements" minOccurs="1" maxOccurs="1"/>		
		<xs:element name="defaultStatement" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="threadNumRange" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>		
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_bindExpr">
	<xs:sequence>
		<xs:element name="paramList" type="T_listVariablePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="call" type="T_callExprPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_callExpr">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="function" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="arguments" type="T_listExpressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_castExpr">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="subExpression" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_memberAccessExpr">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1"/>
		<xs:element name="subExpression" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="memberName" type="T_identifierPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_tupleProjectionExpr">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="subExpression" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="index" type="xs:integer" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_vectorExpr">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="expressions" type="T_listExpressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_structExpr">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="members" type="T_members" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_unionExpr">
	<xs:sequence>
		<xs:element name="type" type="T_typePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="memberName" type="T_identifierPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="member" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_declarationStmt">
	<xs:sequence>
		<xs:element name="variable" type="T_variablePtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="expression" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>	
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_compoundStmt">
	<xs:sequence>
		<xs:element name="statements" type="T_listStatementPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_whileStmt">
	<xs:sequence>
		<xs:element name="condition" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="body" type="T_statementPtr" minOccurs="1" maxOccurs="1"/>	
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_switchStmt">
	<xs:sequence>
		<xs:element name="expression" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
		<xs:element name="cases" type="T_cases" minOccurs="1" maxOccurs="1"/>
		<xs:element name="defaultCase" type="T_statementPtr" minOccurs="1" maxOccurs="1"/>		
		<xs:element name="annotations" type="T_annotations" minOccurs="0" maxOccurs="1"/>	
	</xs:sequence>
		<xs:attribute name="id" type="xs:integer" use="required"/>
</xs:complexType>

<xs:complexType name="T_ifStmt">
	<xs:sequence>
		<xs:element name="condition" type="T_expressionPtr" minOccurs="1" maxOccurs="1"/>
